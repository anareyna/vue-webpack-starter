{
  "name": "vue-webpack-simple",
  "description": "A Vue.js project | node_modules/.bin/et -i element-my-theme.css para descargar el tema por default de element",
  "version": "1.0.0",
  "author": "",
  "private": true,
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --check-coverage --lines 70 mocha-webpack --webpack-config webpack.test.config.js test --recursive --require test/.setup",
    "lint": "eslint --ext .js,.vue src",
    "server": "json-server --watch ./database.json --routes routers.json --port 3004",
    "element": "node_modules/.bin/et -c src/concessionaire/library/toolbox/element-modifiers/_element-variables-my-theme.css -o src/concessionaire/library/toolbox/element-modifiers/my-theme",
    "local": "export NODE_ENV=local && webpack --watch",
    "watch": "export NODE_ENV=watch && webpack --watch",
    "dev": "export NODE_ENV=dev && webpack --hide-modules",
    "pre": "export NODE_ENV=pre && webpack --hide-modules",
    "pre:version": "export VERSION=true NODE_ENV=pre && webpack --hide-modules",
    "prod": "export NODE_ENV=prod && webpack --hide-modules",
    "dummy": "export NODE_ENV=dummy && webpack --hide-modules",
    "hash": "version=true yarn"
  },
  "nyc": {
    "exclude": [
      ".tmp",
      "test"
    ],
    "instrument": false,
    "sourceMap": false
  },
